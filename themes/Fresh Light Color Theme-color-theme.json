		"activityBar.background": "#303030",
		"activityBar.foreground": "#FFFFFF",
		"activityBar.inactiveForeground": "#C7C7C7",
		"activityBarBadge.background": "#5FC9F8",
		"activityBarBadge.foreground": "#FFFFFF",
		"badge.background": "#5FC9F8",
		"badge.foreground": "#FFFFFF",
		"editor.background": "#DDDDDD",
		"editor.foreground": "#353535",
		"editor.inactiveSelectionBackground": "#35353515",
		"editor.lineHighlightBackground": "#FFFFFF90", 
		"editor.lineHighlightBorder": "#FFFFFF10",
		"editor.selectionBackground": "#35353535",
		"editor.selectionHighlightBackground": "#FFFFFF50",
		"editorCursor.foreground": "#353535",
		"editorHoverWidget.background": "#DDDDDD",
		"editorHoverWidget.border": "#353535",
		"editorHoverWidget.foreground": "#353535",
		"editorLineNumber.activeForeground": "#454545",
		"editorLineNumber.foreground": "#AAAAAA",
		"list.activeSelectionBackground": "#AAAAAA",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.inactiveSelectionBackground": "#BCBCBC",
		"list.inactiveSelectionForeground": "#FFFFFF",
		"list.hoverBackground": "#BCBCBC",
		"list.hoverForeground": "#FFFFFF",
		"sideBarTitle.foreground": "#353535",
		"sideBarSectionHeader.background": "#DDDDDD",
		"sideBarSectionHeader.foreground": "#353535",
		"sideBar.background": "#EEEEEE",
		"sideBar.foreground": "#353535",
		"statusBar.background": "#235997", 
		"statusBar.debuggingBackground": "#FF9D6B",
		"statusBar.noFolderBackground": "#AE56C4",
		"tab.inactiveBackground": "#EEEEEE",
	},
	"tokenColors": [
		{
			"name": "Class",
			"scope": [
				"support.class",
				"support.type",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8495FE",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#AAAAAA",
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
			],
			"settings": {
				"foreground": "#36A5FE",
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#B98913",
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.other",
				"keyword.other.import",
			],
			"settings": {
				"foreground": "#14A6C1",
			}
		},
		{
			"name": "Keyword | Assignment Operator",
			"scope": [
				"keyword.operator.assignment",
			],
			"settings": {
				"foreground": "#555555",
			}
		},
		{
			"name": "Keyword | Control",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#36C285",
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.modifier",
			],
			"settings": {
				"foreground": "#FF61A5",
			}
		},
		{
			"name": "Storage | Primitive Type",
			"scope": [
				"storage.type.primitive",
				"storage.type",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#14A6C1",
			}
		},
		{
			"name": "String",
			"scope": [
				"string.interpolated.double",
				"string.interpolated.single",
			],
			"settings": {
				"foreground": "#2DB3FC",
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#555555",
			},
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#353535",
			}
		},
		{
			"name": "Variable | Language",
			"scope": [
				"variable.language",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFA54F",
			}
		},
		// Dart
		{
			"name": "Dart | Operator | Comparison",
			"scope": [
				"keyword.operator.comparison.dart",
			],
			"settings": {
				"foreground": "#14A6C1",
			}
		},
		{
			"name": "Dart | Punctuation | Comma",
